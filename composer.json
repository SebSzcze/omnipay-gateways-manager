{
    "name": "lari/omnipay-payment-manager",
    "description": "Paymnet Manager System for Omnipay",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Szczepanski",
            "email": "sebastian@ably.pl"
        }
    ],
    "require": {
        "webpatser/laravel-uuid": "^3.0",
        "php-http/guzzle6-adapter": "^1.1",
        "omnipay/common": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "^3.6",
        "symfony/var-dumper": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "Lari\\Payments\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Lari\\Payments\\PaymentManagerServiceProvider"
            ]
        }
    }
}
